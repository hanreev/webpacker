{
  "rules": {
    "border-zero": 0,
    "class-name-format": 0,
    "force-attribute-nesting": 0,
    "force-element-nesting": 0,
    "force-pseudo-nesting": 0,
    "hex-length": 0,
    "indentation": [
      1,
      {
        "size": 4
      }
    ],
    "nesting-depth": [
      1,
      {
        "max-depth": 6
      }
    ],
    "no-color-keywords": 0,
    "no-color-literals": 0,
    "no-css-comments": 0,
    "no-ids": 0,
    "no-important": 0,
    "no-qualifying-elements": 0,
    "no-transition-all": 0,
    "no-url-domains": 0,
    "no-url-protocols": 0,
    "no-vendor-prefixes": 1,
    "placeholder-in-extend": 0,
    "property-sort-order": 0,
    "quotes": 1,
    "url-quotes": 0
  }
}
