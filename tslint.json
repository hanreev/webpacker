{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "curly": [true, "as-needed"],
    "interface-name": false,
    "member-access": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "variable-name": false,
    "array-type": false
  }
}
